#include <stdio.h>
 
int main() {
    long int x=0;
    int n=0, countd=0, countf=0, i=0;
    scanf("%d", &n);
    
    for(i=0; i<n; i++){
        scanf("%ld", &x);
        if(x>=10&&x<=20){
            countd=countd+1;
        }else{
            countf=countf+1;
        }
    }
    printf("%d in\n%d out\n", countd, countf);
    return 0;
}
