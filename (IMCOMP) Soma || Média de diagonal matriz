#include <stdio.h>
 
int main() {
  char op='\0';
  int i=1, j=0;
  double soma=0, count=0;
  scanf("%c", &op);
  if(op=='S'||op=='M'){//VERIFICA SE ENTROU 'S' OU 'M'
    double s1[12][12];
      for(i=0; i<12; i++){
        for(j=0; j<12; j++){
          if(i==11&&j==11){
            scanf("%lf", &s1[i][j]);
          } 
          if(i!=11||j!=11){
            scanf("%lf\n", &s1[i][j]); 
          }       
        }
        j=0;
      }
    
    j=0;
    for(j=0; j<11; j++){//LOOP DE SOMA DE VALORES INFERIORES A DIAGONAL
      while(i<12){
        soma=soma+s1[i][j];
        count=count+1;
        i=i+1;
      }
      i=1+j;
    }
    if(op=='M'){
      soma=soma/count;
    }
    printf("%.1lf\n", soma);
  } 
  return 0;
}
