#include <stdio.h>
 
int main() {
    double a=0, soma=0, count=0;
    int i=0;
    
    for(i=0; i<6; i++){
        scanf("%lf", &a);
        if(a>0){
            count=count+1;
            soma=soma+a;
        }
    }
    printf("%.0lf valores positivos\n%.1lf\n", count, soma/count);
    return 0;
}
